{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/react"
    },
    {
      "src": "api/voitures.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/voitures.js"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/client/build/$1"
    }
  ]
}
